{
  "name": "new-pwa",
  "version": "0.1.0",
  "scripts": {
    "ng": "ng",
    "serve": "npm run serve:dev",
    "serve:dev": "ng serve --configuration=dev --host 0.0.0.0 --port 4200",
    "serve:qa": "ng serve --configuration=qa --host 0.0.0.0 --ssl true",
    "serve:prod": "ng serve --configuration=production",
    "build:dev": "ionic build --configuration=dev",
    "build:qa": "ionic build --configuration=dev",
    "build:prod": "ionic build --prod --configuration=production",
    "build:dev:android": "ionic cordova build android --configuration=dev",
    "build:prod:android": "ionic cordova build android --release --prod --configuration=production",
    "build:qa:android": "ionic cordova build android --configuration=dev",
    "build:dev:ios": "ionic cordova build ios --configuration=dev --buildFlag=\"-UseModernBuildSystem=0\"",
    "build:qa:ios": "ionic cordova build ios --configuration=dev --buildFlag=\"-UseModernBuildSystem=0\"",
    "install:android": "adb install -r ./platforms/android/app/build/outputs/apk/debug/app-debug.apk",
    "install:ios": "open -a Xcode ./platforms/ios/*.xcodeproj",
    "build": "ng build",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "test": "ng test"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~11.2.8",
    "@angular/common": "~11.2.8",
    "@angular/compiler": "~11.2.8",
    "@angular/core": "~11.2.8",
    "@angular/forms": "~11.2.8",
    "@angular/platform-browser": "~11.2.8",
    "@angular/platform-browser-dynamic": "~11.2.8",
    "@angular/router": "~11.2.8",
    "@capacitor/core": "2.4.7",
    "@ionic/angular": "5.5.2",
    "rxjs": "~6.6.0",
    "tslib": "^2.0.0",
    "zone.js": "~0.11.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1102.7",
    "@angular/cli": "~11.2.7",
    "@angular/compiler": "~11.2.0",
    "@angular/compiler-cli": "~11.2.8",
    "@angular/language-service": "~11.2.0",
    "@capacitor/cli": "2.4.7",
    "@ionic/angular-toolkit": "^3.1.1",
    "@types/jasmine": "~3.6.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "codelyzer": "^6.0.0",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.1.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "protractor": "~7.0.0",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~4.1.5"
  }
}
